  </ColumnSet>
  <SimpleCodeList>
    <Row>
      <Value ColumnRef="code">
        <SimpleValue>AD</SimpleValue>
      </Value>
      <Value ColumnRef="nombre">
        <SimpleValue>Admitido</SimpleValue>
      </Value>
      <Value ColumnRef="name">
        <SimpleValue>Accepted</SimpleValue>
      </Value>
    </Row>
    <Row>
      <Value ColumnRef="code">
        <SimpleValue>RP</SimpleValue>
      </Value>
      <Value ColumnRef="nombre">
        <SimpleValue>Admitido provisionalmente</SimpleValue>
      </Value>
      <Value ColumnRef="name">
        <SimpleValue>Provisionally accepted</SimpleValue>
      </Value>
    </Row>
    <Row>
      <Value ColumnRef="code">
        <SimpleValue>RE</SimpleValue>
      </Value>
      <Value ColumnRef="nombre">
        <SimpleValue>Excluido</SimpleValue>
      </Value>
      <Value ColumnRef="name">
        <SimpleValue>Refused</SimpleValue>
      </Value>
    </Row>
  </SimpleCodeList>
</gc:CodeList>
